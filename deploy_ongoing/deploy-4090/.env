# Grid / carbon
EM_TOKEN=         # (optional) ElectricityMaps API token
EM_ZONE=TW        # your grid zone (e.g. TW for Taiwan)
DEFAULT_KGCO2_PER_KWH=0.45

# Router policy
CRE_THRESHOLD_PCT_PER_G=0.05
UPLIFT_7B=0.25             # expected Δaccuracy when moving from 1.5B → 7B
UPLIFT_GPT5=0.45           # expected Δaccuracy when moving from 7B → GPT-5
DEFAULT_7B_WH=3.0          # Wh used until rolling average is learned
GPT5_G_CO2_PER_1K_TOK=120  # grams / 1k tokens (policy estimate)
SAMPLE_INTERVAL_S=0.2

# OpenAI (for GPT-5 escalation)
OPENAI_API_KEY=

# Model mount points inside containers
SOLVER_MODEL=/models/math-1p5b-awq
ESCALATE_MODEL=/models/qwen2.5-math-7b-awq

# vLLM tuning
SOLVER_GPU_UTIL=0.35       # keep low since both live on one 4090
ESCALATE_GPU_UTIL=0.80
MAX_MODEL_LEN=8192
MAX_BATCHED_TOKENS=4096
MAX_NUM_SEQS=64
SPEC_DRAFT_TOKENS=5